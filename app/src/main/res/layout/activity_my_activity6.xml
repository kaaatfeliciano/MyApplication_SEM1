<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:id="@+id/action_settings"
    android:background="#ffffe506"
    tools:context="com.example.kaaatfeliciano.myapplication.MyActivity6">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Information System"
        android:id="@+id/isdetail"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="An Introduction to Relational Database Theory"
        android:id="@+id/bk1"
        android:enabled="true"
        android:clickable="true"
        android:layout_below="@+id/isdetail"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A Guide To ERP"
        android:id="@+id/bk2"
        android:layout_below="@+id/bk1"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:clickable="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bk3"
        android:layout_below="@+id/bk2"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:clickable="true"
        android:text="Business Information Management" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Technology-based Entrepreneurship"
        android:id="@+id/bk4"
        android:layout_below="@+id/bk3"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:clickable="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Business Information System"
        android:id="@+id/bk5"
        android:layout_below="@+id/bk4"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:clickable="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Making The Most Of Big Data"
        android:id="@+id/bk6"
        android:layout_below="@+id/bk5"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:clickable="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Exercise On Relational Database Theory"
        android:id="@+id/bk7"
        android:layout_below="@+id/bk6"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:clickable="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Business Information Management: Exercise"
        android:id="@+id/bk8"
        android:layout_below="@+id/bk7"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:clickable="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Semantic Web And Ontology"
        android:id="@+id/bk9"
        android:layout_below="@+id/bk8"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:clickable="true" />
</RelativeLayout>
